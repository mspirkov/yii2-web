{
    "name": "mspirkov/yii2-web",
    "description": "Yii2 Web extension.",
    "keywords": [
        "yii",
        "yii2",
        "web",
        "cookie"
    ],
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "MIT",
    "authors": [
        {
            "name": "Maksim Sprikov",
            "email": "spirkov.2001@mail.ru"
        }
    ],
    "scripts": {
        "lint:check": "./vendor/bin/php-cs-fixer check",
        "lint:fix": "./vendor/bin/php-cs-fixer fix",
        "phpstan:analyse": "./vendor/bin/phpstan analyse",
        "rector:check": "./vendor/bin/rector process --dry-run",
        "rector:fix": "./vendor/bin/rector process"
    },
    "require": {
        "php": ">=7.4",
        "yiisoft/yii2": "^2.0.53"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6",
        "friendsofphp/php-cs-fixer": "^3.89",
        "phpstan/phpstan": "^2.1",
        "phpstan/phpstan-phpunit": "^2.0",
        "rector/rector": "^2.2"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "autoload": {
        "psr-4": {
            "MSpirkov\\Yii2\\Web\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MSpirkov\\Yii2\\Web\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "yiisoft/yii2-composer": true
        }
    }
}
